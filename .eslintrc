{
  "extends": ["react-app", "airbnb", "plugin:jsx-a11y/recommended"],
  "plugins": ["jsx-a11y"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-underscore-dangle": 0,
    "func-names": 0,
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0
  },
  "globals": {
    "CONSTANTS": "readonly",
    "log": "readonly"
  }
}
